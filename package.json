    "react-easy-transition": "0.0.5",
    "react-helmet": "^3.1.0",
    "react-router": "^2.7.0",
    "react-router-scroll": "^0.3.2",
    "react-scroll": "^1.4.0",
    "axios": "^0.14.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
